TOKEN "batting_percentage_over_time_endpoint_read_7207" READ

NODE endpoint
DESCRIPTION >
    undefined

SQL >

    %
        SELECT
          game_date,
          sum(stat_hits)/sum(stat_at_bats) AS batting_percentage
        FROM baseball_game_stats
        WHERE player_name = {{String(player_name, 'JAMES VAZQUEZ', required=True)}}
        GROUP BY game_date
        ORDER BY game_date ASC


